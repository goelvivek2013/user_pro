<%= form_for @profile do |f| %>
  <div class="flow-control">
    <%= f.label :name %><br />
    <%= f.text_field :name %><br />
  </div>

  <div class="flow-control">
    <%= f.label :address %><br />
    <%= f.text_field :address %><br />
  </div>

  <div class="flow-control">
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %><br />
  </div>

  <%= f.submit %>
<% end %>